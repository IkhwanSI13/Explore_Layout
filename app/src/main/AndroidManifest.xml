        <activity android:name=".uI.ChipActivity" />
        <activity android:name=".uI.buttomSheet.ButtomSheetActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".uI.ButtomAppBarActivity" />
