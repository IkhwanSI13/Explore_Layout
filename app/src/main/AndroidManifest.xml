<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.ikhwankoto.layout">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".UI.RatingBarActivity"></activity>
        <activity android:name=".UI.MaterialButtonActivity"></activity>
        <activity android:name=".UI.ChipActivity"></activity>
        <activity android:name=".UI.ButtomSheet.ButtomSheetActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.ButtomAppBarActivity" />
        <activity android:name=".UI.TextInputActivity" />
    </application>

</manifest>